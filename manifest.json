{
  "manifest_version": 2,
  "name": "Booky",
  "version": "1.0",
  "description": "A bookmark manager extension for browsers.",
  "permissions": ["storage", "tabs"],
  "browser_action": {
    "default_popup": "newtab.html",
    "default_icon": {
      "16": "images/favicon-16x16.png",
      "32": "images/favicon-32x32.png",
      "48": "images/favicon-96x96.png",
      "128": "images/favicon-128.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "images/favicon-16x16.png",
    "32": "images/favicon-32x32.png",
    "48": "images/favicon-96x96.png",
    "128": "images/favicon-128.png"
  }
}
